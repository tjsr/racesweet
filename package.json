{
  "name": "racesweet",
  "version": "0.0.1",
  "description": "Race entry and results utilities",
  "keywords": [
    "Racing",
    "Timing",
    "Results"
  ],
  "license": "ISC",
  "author": "Tim Rowe <tim@tjsr.id.au>",
  "type": "commonjs",
  "main": ".webpack/main",
  "old.main": "index.mjs",
  "vite.main": ".vite/build/main.js",
  "scripts": {
    "build": "babel index.mjs -d build",
    "clean": "rimraf src/generated build dist",
    "lint": "npm run lint:ts && npm run lint:json",
    "lint:json": "jsonlint src/**/*.json",
    "lint:ts": "eslint",
    "serve": "webpack --config webpack.main.config.ts src/app/index",
    "start": "electron-forge start",
    "test": "vitest",
    "test:table": "tsx ./src/printCrossings.ts",
    "types": "mkdirp src/generated/typebox && ts2typebox --input src/model/index.ts --output src/generated/typebox/index.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.27.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@tjsr/eslint-config": "github:tjsr/eslint-config#main",
    "@tsconfig/node-ts": "^23.6.1",
    "@tsconfig/node22": "^22.0.1",
    "@types/date-fns": "^2.5.3",
    "@types/node": "^22.13.17",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^10.0.0",
    "chokidar": "^4.0.3",
    "css-loader": "^7.1.2",
    "electron": "^36.4.0",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.23.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "mkdirp": "^3.0.1",
    "node-loader": "^2.1.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts2typebox": "^1.6.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "any-date-parser": "^2.1.0",
    "cli-table3": "^0.6.5",
    "cli-tableau": "^2.0.1",
    "colors": "^1.4.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "electron-debug": "^4.1.0",
    "electron-timber": "^1.0.0",
    "node-adodb": "^5.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=11.4.1"
  }
}
